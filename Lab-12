import pandas as pd
import numpy as np

np.random.seed(2)
df = pd.DataFrame(
    np.random.randint(1, 100, size=(10, 4)),
    columns=['A', 'B', 'C', 'D']
)

def black_background_yellow_font(val):
    return 'color: yellow; background-color: black'

styled_df = df.style.map(black_background_yellow_font)

styled_df
