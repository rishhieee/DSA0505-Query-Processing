import matplotlib.pyplot as plt

data = [
    [1, 2],
    [2, 4],
    [3, 1]
]

x = [row[0] for row in data]
y = [row[1] for row in data]

plt.plot(x, y, marker='o', linestyle='-', color='red', label='Test Line')

plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.title('Line Plot from Hardcoded Data')

plt.legend()

plt.show()
