import matplotlib.pyplot as plt
import numpy as np

# Generate random data
np.random.seed(0)  # for reproducibility
x = np.random.rand(50)  # 50 random numbers between 0 and 1
y = np.random.rand(50)

# Create scatter plot with empty circles
plt.figure(figsize=(8,6))
plt.scatter(x, y, facecolors='none', edgecolors='blue', s=80, linewidths=1.5)

# Labels and title
plt.xlabel('Random x values')
plt.ylabel('Random y values')
plt.title('Scatter Plot with Empty Circles')

# Show grid
plt.grid(True)

# Display plot
plt.show()
